{
  "name": "react-app-template",
  "version": "0.0.0",
  "description": "A React app template scaffolded by CRA.",
  "author": "Mark <mark@remarkablemark.org>",
  "scripts": {
    "build": "NODE_ENV=production react-app-rewired build",
    "clean": "rimraf build",
    "deploy": "scripts/deploy.sh",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "release": "standard-version --no-verify",
    "start": "NODE_ENV=development react-app-rewired start",
    "test": "npm run test:watch -- --ci --coverage",
    "test:watch": "react-app-rewired test --env=jsdom"
  },
  "homepage": "https://remarkablemark.github.io/react-app-template/",
  "devDependencies": {
    "@commitlint/cli": "7.2.1",
    "@commitlint/config-conventional": "7.1.2",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "eslint-plugin-prettier": "3.0.0",
    "gitploy": "0.1.0",
    "husky": "1.3.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "react": "16.7.0",
    "react-app-rewired": "1.6.2",
    "react-dom": "16.7.0",
    "react-hot-loader": "4.6.3",
    "react-scripts": "1.1.4",
    "rimraf": "2.6.2",
    "standard-version": "4.4.0"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5.3"
  },
  "private": true,
  "license": "MIT"
}
